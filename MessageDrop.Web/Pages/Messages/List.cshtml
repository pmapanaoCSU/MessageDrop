@page
@model MessageDrop.Web.Pages.Messages.ListModel
@{
}

<h1>Messages</h1>


<table class="table">
    <tr>
        <th>ID</th>
        <th>Message</th>
    </tr>
    @foreach (var message in Model.Messages)
    {
        <tr>
            <td>@message.Id</td>
            <td>@message.MessageString</td>

        </tr>

    }
</table>
